   {
      "name": "AO3 Blocker",
      "version": "1.0",
      "manifest_version": 3,
      "content_scripts": [
        {
          "matches": [
            "https://archiveofourown.org/*"
          ],
          "js": ["src/content.js"]
        }
      ],
      "background": {
        "service_worker":  "src/background.js" 
      },
      "permissions": [
        "contextMenus",
        "storage"
      ],
      "options_page": "src/options.html",
      "icons": {
        "48": "icons/ao3block48.png",
        "128": "icons/ao3block128.png"
      }
    }
