   {
      "name": "AO3 Blocker",
      "version": "1.0",
      "manifest_version": 2,
      "content_scripts": [
        {
          "matches": [
            "https://archiveofourown.org/*"
          ],
          "js": ["src/content.js"]
        }
      ],
      "background": {
        "scripts":  ["src/background.js"]
      },
      "permissions": [
        "contextMenus",
        "storage"
      ],
      "options_ui": {
        "page": "src/options.html",
        "browser_style": true
      },
      "icons": {
        "48": "icons/ao3block48.png",
        "128": "icons/ao3block128.png"
      },
      "browser_specific_settings": {
        "gecko": {
          "id": "366e86ede653f924da5245147285a4f61ffb4383@addon",
          "strict_min_version": "42.0"
        }
      }
    }
